# Vercel Blob Storage 설정 (사용 중)
BLOB_READ_WRITE_TOKEN=your-blob-read-write-token

# 이미지 서비스 선택 (vercel-blob 또는 aws-s3)
IMAGE_SERVICE=vercel-blob

# AWS S3 설정 (선택사항 - S3 사용 시)
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
S3_BUCKET_NAME=donggori-images

# CloudFront CDN 설정 (선택사항)
CLOUDFRONT_DOMAIN=your-cloudfront-domain.cloudfront.net

# 네이버맵 API 설정
NEXT_PUBLIC_NAVER_MAP_CLIENT_ID=your-naver-map-client-id

# Clerk 인증 설정 (Pro 플랜)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your-clerk-publishable-key
CLERK_SECRET_KEY=your-clerk-secret-key

# Clerk 도메인 설정 (Pro 플랜에서 Allowlist 사용 시)
CLERK_ALLOWED_EMAIL_DOMAINS=gmail.com,naver.com,kakao.com,faddit.co.kr 